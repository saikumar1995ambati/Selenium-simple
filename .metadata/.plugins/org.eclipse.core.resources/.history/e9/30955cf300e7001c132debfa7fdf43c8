package JavaCoding;

public class SecondLargest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [] arr = {1, 5, 50, 19,30,40};
		
		secondNum(arr);

	}

	private static void secondNum(int[] arr) {
		
		int n = arr.length;
		int largest = arr[0];
		//int second = arr[0];
		for(int i=0;i<=n-1;i++)
		
			{
				if(largest>arr[i])
				{
				 largest=arr[i];
							
				}
				
		}
		System.out.println(largest);
	}

}
