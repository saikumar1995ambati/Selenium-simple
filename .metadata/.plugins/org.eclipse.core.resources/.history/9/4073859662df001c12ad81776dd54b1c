package JavaCoding;

public class Sumnumber {
	static int num = 123;
	static int gnum = num;

	public static void main(String[] args) {

		System.out.println(sumofNumber(num));

	}

	private static int sumofNumber(int num2) {
		while (num2 != 0) {
			int sum = 0;
			System.out.println(num2);
			int rem = num2 % 10;
			sum = (int) (sum + Math.pow(rem, 3));
			num2 = num2 / 10;

		}
		return sum;

	}

}
