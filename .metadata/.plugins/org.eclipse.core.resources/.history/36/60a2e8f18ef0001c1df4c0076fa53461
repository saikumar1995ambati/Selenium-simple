package com.saikumar.practice;

public class ArrayAdd {

	public static void main(String[] args) {
		int a [] = new int[] {1, 5, 11, 12, 14};
		
		int n = 17;
		twonum(a,n);
		

	}

	private static void twonum(int[] a, int n) {
		for(int i=0;i<a.length;i++)
		{
			
				int sum=a[i]+a[a.length-1-i];
				
				if(sum == n)
				{
					System.out.println(a[i]+ " "+ a[a.length-1-i]);
					
				
					
				}
			}
			
		
	}
}
